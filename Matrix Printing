// WAP TO DECLARE , INITIALIZE AND PRINT A MATRIX OF 3*3


#include <stdio.h>

int main()
{
    //DECLARATION
    int a[3][3] , i , j ;

    // USER INPUT
    printf("Enter the Values of Matrix \n");
    for( i=0 ; i<=2 ; i++)
    {
        for( j=0 ; j<=2 ; j++)
        {
            scanf("\n %d" , &a[i][j]);
        }
        printf("\n");
    }

    //PRINTING
    printf("The Matrix is \n");
    for( i=0 ; i<=2 ; i++)
    {
        for( j=0 ; j<=2 ; j++)
        {
            printf("  %d  " , a[i][j]);
        }
        printf("\n");
    }
    return 0;
}
